{
	"name": "kbot2",
	"version": "2.0.0",
	"private": true,
	"scripts": {
		"production": "NODE_ENV=production ts-node --project ./server/tsconfig.json -r tsconfig-paths/register ./server/src/index.ts",
		"devserver": "ts-node --project ./server/tsconfig.json -r tsconfig-paths/register ./server/src/index.ts",
		"dev": "concurrently -m 2 \"vite --host --port 3000\" \"nodemon\"",
		"build": "yarn build:lint && yarn build:web",
		"build:lint": "yarn lint",
		"build:web": "vite build",
		"lint": "concurrently -m 3 \"yarn lint:server\" \"yarn lint:shared\" \"yarn lint:react\"",
		"lint:server": "eslint ./server/src/**/*.{ts,tsx,js,jsx} --fix",
		"lint:shared": "eslint ./shared/**/*.{ts,tsx,js,jsx} --fix",
		"lint:react": "eslint ./src/**/*.{ts,tsx,js,jsx} --fix"
	},
	"dependencies": {
		"@etothepii/satisfactory-file-parser": "^0.0.31",
		"@kyri123/k-javascript-utils": "^0.2.1-build.12",
		"@kyri123/k-reactutils": "^0.2.9-build.29",
		"bootstrap": "5.3.0-alpha3",
		"compressing": "^1.9.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-fileupload": "^1.4.0",
		"graphql-request": "^6.0.0",
		"jsonwebtoken": "^9.0.0",
		"lodash": "^4.17.21",
		"mongoose": "^7.0.3",
		"react": "^18.2.0",
		"react-bootstrap": "^2.7.2",
		"react-dom": "^18.2.0",
		"react-icons": "^4.8.0",
		"react-markdown": "^8.0.6",
		"react-router-dom": "^6.10.0",
		"react-select": "^5.7.2",
		"sass": "^1.62.0",
		"scss": "^0.2.4",
		"socket.io": "^4.6.1",
		"socket.io-client": "^4.6.1",
		"sweetalert2": "^11.7.3",
		"sweetalert2-react-content": "^5.0.7"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/express": "^4.17.17",
		"@types/express-fileupload": "^1.4.1",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/lodash": "^4.14.194",
		"@types/node": "^18.15.11",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@vitejs/plugin-react": "^3.1.0",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.0.1",
		"eslint": "^8.37.0",
		"eslint-config-react": "^1.1.7",
		"eslint-config-react-app": "^7.0.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.2",
		"nodemon": "^2.0.22",
		"postcss-cli": "^10.1.0",
		"react-error-overlay": "6.0.9",
		"tailwindcss": "^3.3.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"vite": "^4.2.0",
		"vite-plugin-eslint": "^1.8.1"
	}
}
